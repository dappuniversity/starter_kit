{
  "name": "bread-crumbs",
  "private": true,
  "version": "0.0.0",
  "description": "Web plugin to track user navigation between domains",
  "author": "pmjunquera@gmail.com",
  "scripts": {
    "start": "cd ./frontend && dotenv -e ../.env.local -- next dev",
    "serve": "cd ./frontend && next start -p 80",
    "build:dev": "cd ./frontend && dotenv -e ../.env.development next build",
    "build:prod": "cd ./frontend && dotenv -e ../.env.production next build",
    "ganache": "ganache -h 127.0.0.1 -p 8545 -i 1337",
    "truffle:test": "truffle test",
    "truffle:migrate": "truffle migrate",
    "truffle:develop": "truffle develop --log",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev --create-only --name add-client",
    "prisma:migrate:prod": "dotenv -e .env.local -- npx prisma migrate deploy",
    "postgres:deploy": "dotenv -e .env.local -- docker-compose -f ./deploy/local/docker/docker-compose.yml up -d",
    "lint": "eslint ./ --ext .js --ext .jsx .ts --ext .tsx --ext .ms --ext .mdx",
    "lint:fix": "eslint ./ --ext .js --ext .jsx --ext .ts --ext .tsx --ext .ms --ext .mdx --fix"
  },
  "dependencies": {
    "@mantine/core": "^3.6.6",
    "@mantine/hooks": "^3.6.6",
    "@mantine/next": "^3.6.6",
    "@mantine/notifications": "^3.6.6",
    "@openzeppelin/contracts": "^4.4.2",
    "@prisma/client": "^3.9.2",
    "@radix-ui/react-icons": "^1.0.3",
    "@types/clientjs": "^0.1.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-slick": "^0.23.8",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-bignumber": "3.0.0",
    "classnames": "^2.3.1",
    "clientjs": "^0.2.1",
    "dotenv-cli": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ganache-cli": "^6.12.2",
    "generate": "^0.14.0",
    "html-metadata-parser": "^2.0.4",
    "link-preview-generator": "^1.0.7",
    "nanoid": "^3.3.0",
    "next": "12.0.8",
    "prisma": "^3.11.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-scripts": "2.1.3",
    "react-slick": "^0.28.1",
    "sass": "^1.49.7",
    "slick-carousel": "^1.8.1",
    "swr": "^1.2.0",
    "truffle": "5.0.5",
    "truffle-assertions": "^0.9.2",
    "typescript": "^4.6.3",
    "web3": "1.0.0-beta.55"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-mdx": "^1.15.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-standard": "^5.0.0",
    "ganache": "^7.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
